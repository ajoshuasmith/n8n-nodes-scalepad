{
	"name": "n8n-nodes-quoter",
	"version": "1.0.0",
	"description": "n8n community node for Quoter by ScalePad - Quote Management and Automation",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"quoter",
		"scalepad",
		"quotes",
		"msp",
		"quoting"
	],
	"license": "MIT",
	"homepage": "https://github.com/ajoshuasmith/n8n-nodes-scalepad",
	"author": {
		"name": "ScalePad Community",
		"email": "support@scalepad.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ajoshuasmith/n8n-nodes-scalepad.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials",
		"lintfix": "eslint nodes credentials --fix",
		"prepublishOnly": "npm run build && npm run lint"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/ScalePadQuoterOAuth2Api.credentials.js"
		],
		"nodes": [
			"dist/nodes/ScalePadQuoter/ScalePadQuoter.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.29.0",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.0.0",
		"prettier": "^2.7.1",
		"typescript": "^4.9.4"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
